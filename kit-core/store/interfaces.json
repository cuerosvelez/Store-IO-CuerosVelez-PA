{
  "box": {
    "component": "Box",
    "composition": "children"
  },
  "navbar": {
    "component": "NavBar",
    "composition": "children",
    "allowed": ["head-band", "nav-logo", "navigation", "nav-right"]
  },
  "head-band": {
    "component": "HeadBand"
  },
  "nav-logo": {
    "component": "NavLogo"
  },
  "navigation": {
    "component": "Navigation",
    "allowed": ["nav-logo", "search-bar"]
  },
  "nav-right": {
    "component": "NavRight",
    "allowed": ["search-bar", "minicart.v2", "login-content"]
  },
  "whatsapp": {
    "component": "WhatsApp"
  },
  "nav-footer": {
    "component": "Footer"
  },
  "modal-filter": {
    "component": "Filter",
    "allowed": ["filter-navigator.v3"]
  },
  "current-page": {
    "composition": "children",
    "component": "CurrentPage"
  },
  "filter-tags": {
    "component": "FilterTags",
    "composition": "children",
    "allowed": ["breadcrumb.search"]
  },
  "filter-tags-plp": {
    "component": "FilterTagsProducts",
    "composition": "children",
    "allowed": [
      "search-result-layout.desktop",
      "search-result-layout.mobile",
      "search-not-found-layout"
    ]
  },
  "media": {
    "component": "Media"
  },
  "media-button": {
    "component": "MediaButton"
  },
  "list-context.media-list-button": {
    "component": "MediaListButton"
  },
  "list-context.media-list": {
    "component": "MediaList",
    "composition": "children",
    "content": {
      "$ref": "app:vtex.store-media#/definitions/MediaList"
    }
  },
  "product-gallery": {
    "composition": "children",
    "component": "ProductGallery"
  },
  "product-color": {
    "component": "ProductColor"
  },
  "list-context.card": {
    "component": "CardSlider",
    "composition": "children"
  },
  "product-detail": {
    "component": "ProductDetail"
  },
  "condition-header": {
    "component": "ConditionHeader",
    "composition": "children"
  },
  "scroll-up-view": {
    "component": "ScrollUpView",
    "composition": "children"
  },
  "container-reviews": {
    "component": "ContainerReviews",
    "composition": "children"
  },
  "custom-minicart-product-list": {
    "component": "CustomMinicartProductList"
  },
  "keep-buying-link": {
    "component": "KeepingBuying"
  },
  "product-image-slider": {
    "component": "ProductImageSlider"
  },
  "size-guide": {
    "component": "SizeGuide",
    "composition": "children"
  }
}
