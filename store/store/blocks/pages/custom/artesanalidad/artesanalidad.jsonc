                },
                {
                    "image": "https://cuerosvelezco.vtexassets.com/assets/vtex.file-manager-graphql/images/a12016a0-ce63-4056-afb2-3d268b153933___b4f91c631c9a07cda7b6e9e9939fda64.jpg",
                    "mobileImage": "https://cuerosvelezco.vtexassets.com/assets/vtex.file-manager-graphql/images/57ed5c1e-5e71-409d-9239-527d98ecf0d2___b06ecfe9c624e95eb50d4d01b95e4453.jpg",
