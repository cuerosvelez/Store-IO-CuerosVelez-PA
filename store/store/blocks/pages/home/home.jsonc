      "buttonList": {
        "listPositionDesk": 2,
        "listPositionMobile": 2,
        "buttons": [
          {
            "__editorItemTitle": "COMPRAR AHORA",
            "name": "COMPRAR AHORA",
            "url": "/sale/ropa",
            "newTab": false
          }
        ],
        "color": "#ffffff",
        "colorBackground": "#00000040",
        "advancedSettings": {
          // "showAdvancedSettings": true,
          // "marginBottom": 90,
          "marginBottomMobile": 50
        }
      }
    }
  },
  "media-button#categoria-dos": {
    "props": {
      "mediaType": "image",
      "link": {
        "url": "/sale/zapatos"
      },
      "blockClass": ["banner-categoria-dos"],
      "image": "https://cuerosvelezpa.vtexassets.com/assets/vtex.file-manager-graphql/images/cfe6cfa2-2c1b-456c-ab66-825e269bd58e___d4e66f5a408a0885b90c03875a70aad1.jpg",
      "imageMobile": "https://cuerosvelezpa.vtexassets.com/assets/vtex.file-manager-graphql/images/83eda033-3b94-4c27-be13-3ec694013c76___a83c18d82348865524ac69ebec8d7833.jpg",
      "buttonList": {
        "listPositionDesk": 1,
        "listPositionMobile": 2,
        "buttons": [
          {
            "__editorItemTitle": "ROPA",
            "name": "VER ROPA",
            "url": "/sale/zapatos",
            "newTab": false
          }
        ],
        "color": "#ffffff",
        "colorBackground": "#00000040",
        "advancedSettings": {
          "showAdvancedSettings": true,
          // "marginBottom": 90,
          "marginBottomMobile": 50
        }
      }
    }
  },
  "media-button#banner-tres": {
    "props": {
      "mediaType": "image",
      "link": {
        "url": "/activacion-linea"
      },
      "blockClass": ["banner-tres-mujer-human-depto"],
      "image": "https://cuerosvelezpa.vtexassets.com/assets/vtex.file-manager-graphql/images/7dfee8d1-1c7b-4a23-9a95-660ed9cba1e4___e5a0158567517a8dc0b96cca490c6b15.jpg",
      "imageMobile": "https://cuerosvelezpa.vtexassets.com/assets/vtex.file-manager-graphql/images/c156f07a-4b44-43b6-aa7f-b76792335e8e___1cca4d5c375e27b830f7d2eb372769cc.jpg",
      "buttonList": {
        "listPositionDesk": 1,
        "listPositionMobile": 2,
        "buttons": [
          {
            "__editorItemTitle": "COMPRAR AHORA",
            "name": "COMPRAR AHORA",
            "url": "/activacion-linea"
          }
        ],
        "color": "#ffffff",
        "colorBackground": "#00000040",
        "advancedSettings": {
          // "showAdvancedSettings": true,
          "marginBottom": 240,
          "widthContainer": 50,
          "marginBottomMobile": 50
        }
      }
    }
  },
  "box#contenedor-vitrina-home": {
    "props": {
      "tag": "section"
    },
    "children": [
      "rich-text#title-vitrina-home",
      "list-context.product-list#vitrina-home"
    ]
  },
  "rich-text#title-vitrina-home": {
    "props": {
      "text": "## NUESTROS FAVORITOS",
      "blockClass": ["title-vitrina-home"]
    }
  },
  "list-context.product-list#vitrina-home": {
    "blocks": ["product-summary.shelf"],
    "children": ["slider-layout#slider-vitrina-home"],
    "props": {
      "orderBy": "OrderByBestDiscountDESC",
      "collection": "412",
      "maxItems": 30
    }
  },
  "slider-layout#slider-vitrina-home": {
    "props": {
      "itemsPerPage": {
        "desktop": 4,
        "tablet": 2,
        "phone": 2
      },
      "infinite": true,
      "showNavigationArrows": "desktopOnly",
      "showPaginationDots": "always",
      "blockClass": "carrousel-vitrina-home"
    }
  },
  "media-button#banner-cuatro": {
    "props": {
      "mediaType": "image",
      "link": {
        "url": "/calzado-ultimas-tallas"
      },
      "blockClass": ["banner-cuatro"],
      "image": "https://cuerosvelezpa.vtexassets.com/assets/vtex.file-manager-graphql/images/1d7bd00c-7fd1-4dbe-bb85-c48aece59c90___16e1afcecff6b32c51c54621c7a84170.jpg",
      "imageMobile": "https://cuerosvelezpa.vtexassets.com/assets/vtex.file-manager-graphql/images/92a15d36-61ad-4b10-a244-43ab3921a034___d32567693c2788891935f1d77bacb825.jpg",
      "buttonList": {
        "listPositionDesk": 2,
        "listPositionMobile": 2,
        "buttons": [
          {
            "__editorItemTitle": "COMPRAR AHORA",
            "name": "COMPRAR AHORA",
            "url": "/calzado-ultimas-tallas",
            "newTab": false
          }
        ],
        "color": "#ffffff",
        "colorBackground": "#00000040",
        "advancedSettings": {
          // "showAdvancedSettings": true,
          // "marginBottom": 90,
          // "marginBottomMobile": 90
        }
      }
    }
  },
  "box#contenedor-nuestros-servicios": {
    "title": "Contenedor nuestro servicios",
    "props": {
      "blockClass": ["contenedor-nuestros-servicios"]
    },
    "children": [
      "rich-text#title-nuestros servicios",
      "list-context.card#baners-de-servicios"
    ]
  },
  "rich-text#title-nuestros servicios": {
    "props": {
      "text": "NUESTROS SERVICIOS",
      "blockClass": "title-nuestros-servicios"
    }
  },

  "list-context.card#baners-de-servicios": {
    "props": {
      "contentItem": [
        {
          "type": "video",
          "video": "https://cuerosvelezco.myvtex.com/api/dataentities/AA/documents/918/video/attachments/desk-restauracion.mp4",
          "mobileVideo": "https://cuerosvelezco.myvtex.com/api/dataentities/AA/documents/919/video/attachments/mob-restauracion.mp4",
          "text": "**RESTAURACIÓN**\n Una experiencia en tiendas donde se podrá tener la opción de solicitar reparación o mantenimiento de tus productos y darles una segunda vida."
        },
        {
          "type": "video",
          "video": "https://cuerosvelezco.myvtex.com/api/dataentities/AA/documents/914/video/attachments/desk-marcacion.mp4",
          "mobileVideo": "https://cuerosvelezco.myvtex.com/api/dataentities/AA/documents/915/video/attachments/mob-marcacion.mp4",
          "text": "**MARCACIÓN LÁSER**\n Personaliza tus piezas en cuero con nuestro servicio de marcación láser. Busca una tienda con el servicio cerca de tu ubicación y lleva la idea que tienes para que sea marcada en el cuero."
        },
        {
          "type": "video",
          "video": "https://cuerosvelezco.myvtex.com/api/dataentities/AA/documents/916/video/attachments/desk-personalshopper.mp4",
          "mobileVideo": "https://cuerosvelezco.myvtex.com/api/dataentities/AA/documents/917/video/attachments/mob-personalshopper.mp4",
          "text": "**PERSONAL SHOPPER**\n Personal Shopper, es una herramienta gratuita y dispuesta para que eleves al máximo cada pieza que uses, solo debes reservar una cita y asistir a una de nuestras 10 tiendas habilitadas."
        }
      ],
      "blockClasses": ["banner-dos-artesanalidad"]
    },
    "children": ["slider-layout#banners-nuestros-servicios"]
  },
  "slider-layout#banners-nuestros-servicios": {
    "title": "Banner pricipales 4 Slide",
    "props": {
      "itemsPerPage": {
        "desktop": 3,
        "tablet": 2,
        "phone": 1
      },
      "autoplay": {
        "timeout": 5000,
        "stopOnHover": false
      },
      "slideTransition": {
        "speed": 1000,
        "delay": 0,
        "timing": ""
      },
      "infinite": false,
      "showNavigationArrows": "never",
      "showPaginationDots": "never",
      "blockClass": [
        "slider-dotsLine",
        "slider-banners-home",
        "slider-banners-nuestros-servicios"
      ]
    }
  }
}
