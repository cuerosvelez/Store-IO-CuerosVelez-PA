  "media-button#exa": {
    "props": {
      "mediaType": "image",
      "link": {
        "url": "/guia-regalos"
      },
      "blockClass": ["banner-tres-mujer-human-depto"],
      "image": "https://cuerosvelezco.vtexassets.com/assets/vtex.file-manager-graphql/images/4953005d-8d15-4f4e-a752-66e257c8ecce___6e705caba67d598e1f646e5b8ded4fd3.jpg",
