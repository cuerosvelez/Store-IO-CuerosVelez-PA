{
  "store.home": {
    "blocks": ["structured-data", "box#home"],
    "parent": {
      "header": "condition-header",
      "footer": "nav-footer"
    }
  },
  "structured-data": {
    "props": {
      "structuredDataList": [
        {
          "__editorItemTitle": "Datos Logo",
          "json__ld": "{   \"@context\":\"https://schema.org\",   \"@type\": \"Corporation\",   \"name\": \"Velez Panamá\",   \"url\": \"https://www.velez.pa/\",   \"logo\": \"https://cuerosvelezco.vteximg.com.br/arquivos/logo-velez-gris.png\",   \"sameAs\": [     \"https://www.facebook.com/velezartisan/\",     \"https://www.instagram.com/velezartisan/?hl=es\",     \"https://www.youtube.com/@velezoficial\",     \"https://www.linkedin.com/company/v%C3%A9lez-artisan/posts/?feedView=all\",     \"https://co.pinterest.com/velezartisan/\"   ] }"
        }
      ]
    }
  },
  "box#home": {
    "children": [
      "list-context.media-list-button#banner-principal",
      "slider-layout#banner-text-home",
      "box#banners-secundarios",
