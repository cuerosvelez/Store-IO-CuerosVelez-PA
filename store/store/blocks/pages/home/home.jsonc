      "buttonList": {
        "listPositionDesk": 2,
        "listPositionMobile": 2,
        "buttons": [
          {
            "__editorItemTitle": "ROPA",
            "name": "DESCRUBRE MÁS",
            "url": "/lo-nuevo/ropa",
            "newTab": false
          }
        ],
        "color": "#ffffff",
        "colorBackground": "#ffffff40",
        "avancedSettings": {
          "showAdvancedSettings": true,
          "marginBottom": 90,
          "marginBottomMobile": 90
        }
      }
    }
  },
  "slider-layout#banner-text-home": {
    "props": {
      "itemsPerPage": {
        "desktop": 3,
        "tablet": 1,
        "phone": 1
      },
      "infinite": true,
      "showNavigationArrows": "never",
      "showPaginationDots": "never",
      "slideTransition": {
        "speed": 1000,
        "delay": 0,
        "timing": ""
      },
      "autoplay": {
        "timeout": 5000,
        "stopOnHover": true
      },

      "blockClass": "banner-text-home"
    },
    "children": ["rich-text#1", "rich-text#2", "rich-text#3"]
  },
  "rich-text#1": {
    "props": {
      "text": "Envíos en hasta 3 días hábiles",
      "blockClass": "text-banner-home"
    }
  },
  "rich-text#2": {
    "props": {
      "text": "Envío grátis por compras superiores a 100 USD",
      "blockClass": ["text-banner-home", "text-banner-home-medio"]
