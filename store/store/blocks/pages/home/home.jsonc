    "title": "Banner principal",
    "children": ["slider-layout#banners-home"],
    "props": {
      "mediaList": [
        {
          "mediaType": "image",
          "link": {
            "url": "/lo-nuevo",
            "attributeNofollow": false
          },
          "loading": "eager",
          "blockClass": ["banner-tres-mujer-human-deptos"],
          "alt": "Nueva colección",
          "image": "https://cuerosvelezpa.vtexassets.com/assets/vtex.file-manager-graphql/images/a306f8cd-d07a-4536-958b-1b1de7a31cf0___375a46b68ecb535ec32efbecb629d621.jpg",
          "imageMobile": "https://cuerosvelezpa.vtexassets.com/assets/vtex.file-manager-graphql/images/ea4befc4-d816-4ecb-99d3-145c934a0f06___35a41413fa1bb77faf24acf7628833ca.jpg",
          "buttonList": {
            "listPositionDesk": 2,
            "listPositionMobile": 1,
            "buttons": [
              {
                "url": "/lo-nuevo",
                "newTab": false,
                "name": "VER NUEVA COLECCIÓN"
              }
            ],
            "color": "#ffffff",
            "colorBackground": "#3b3a3a4d",
            "advancedSettings": {
              "marginBottomMobile": 50,
              "justify": "justify-center",
              "leftWidthContainer": false,
              "directionRow": false
            },
            "active": true,
            "listPositionDesktop": 1
          }
        }
      ]
    }
  },

  "slider-layout#banner-text-home": {
    "props": {
      "itemsPerPage": {
        "desktop": 3,
        "tablet": 1,
        "phone": 1
      },
      "infinite": true,
      "showNavigationArrows": "never",
      "showPaginationDots": "never",
      "slideTransition": {
        "speed": 1000,
        "delay": 0,
        "timing": ""
      },
      "autoplay": {
        "timeout": 5000,
        "stopOnHover": true
      },

      "blockClass": "banner-text-home"
    },
    "children": ["rich-text#1", "rich-text#2", "rich-text#3"]
  },
  "rich-text#1": {
    "props": {
      "text": "Envíos en hasta 3 días hábiles",
      "blockClass": "text-banner-home"
    }
  },
  "rich-text#2": {
    "props": {
      "text": "Envío gratis por compras superiores a 100 USD",
      "blockClass": ["text-banner-home", "text-banner-home-medio"]
    }
  },
  "rich-text#3": {
    "props": {
      "text": "Cambios y devoluciones gratis",
      "blockClass": "text-banner-home"
    }
  },
