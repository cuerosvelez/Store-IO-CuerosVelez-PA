{
  "store.home": {
    "blocks": ["structured-data", "box#home"],
    "parent": {
      "header": "condition-header",
      "footer": "nav-footer"
    }
  },
  "structured-data": {
    "props": {
      "structuredDataList": [
        {
          "__editorItemTitle": "Datos Logo",
          "json__ld": "{   \"@context\":\"https://schema.org\",   \"@type\": \"Corporation\",   \"name\": \"Velez Panamá\",   \"url\": \"https://www.velez.pa/\",   \"logo\": \"https://cuerosvelezco.vteximg.com.br/arquivos/logo-velez-gris.png\",   \"sameAs\": [     \"https://www.facebook.com/velezartisan/\",     \"https://www.instagram.com/velezartisan/?hl=es\",     \"https://www.youtube.com/@velezoficial\",     \"https://www.linkedin.com/company/v%C3%A9lez-artisan/posts/?feedView=all\",     \"https://co.pinterest.com/velezartisan/\"   ] }"
        }
      ]
    }
  },
  "box#home": {
    "children": [
      "media-button#principal",
      "slider-layout#banner-text-home",
      "box#banners-secundarios",
      "media-button#banner-tres",
      "box#contenedor-vitrina-home",
      "media-button#banner-cuatro",
      "box#contenedor-nuestros-servicios"
    ]
  },
  "media-button#principal": {
    "props": {
      "mediaType": "image",
      "link": {
        "url": "/sale"
      },
      "blockClass": ["banner-tres-mujer-human-depto"],
      "image": "https://cuerosvelezpa.vtexassets.com/assets/vtex.file-manager-graphql/images/83e28f0b-6639-4a19-8d6d-f57ba9bc7aef___2e0cb507ba3386ad0fe16867887acb77.jpg",
      "imageMobile": "https://cuerosvelezpa.vtexassets.com/assets/vtex.file-manager-graphql/images/f17a44d7-bd1d-4301-8e22-cff062502aa3___d7425b18a950540e0c15728d1d046515.jpg",
      "buttonList": {
        "listPositionDesk": 2,
        "listPositionMobile": 2,
        "buttons": [
          {
            "__editorItemTitle": "VER DESCUENTOS",
            "name": "VER DESCUENTOS",
            "url": "/sale",
            "newTab": false
          }
        ],
        "color": "#ffffff",
        "colorBackground": "#3b3a3a4d",
        "advancedSettings": {
          // "showAdvancedSettings": true,
          // "marginBottom": 90,
          "marginBottomMobile": 50
        }
      }
    }
  },
  "slider-layout#banner-text-home": {
    "props": {
      "itemsPerPage": {
        "desktop": 3,
        "tablet": 1,
        "phone": 1
      },
      "infinite": true,
      "showNavigationArrows": "never",
      "showPaginationDots": "never",
      "slideTransition": {
        "speed": 1000,
        "delay": 0,
        "timing": ""
      },
      "autoplay": {
        "timeout": 5000,
        "stopOnHover": true
      },

      "blockClass": "banner-text-home"
    },
    "children": ["rich-text#1", "rich-text#2", "rich-text#3"]
  },
  "rich-text#1": {
    "props": {
      "text": "Envíos en hasta 3 días hábiles",
      "blockClass": "text-banner-home"
    }
  },
  "rich-text#2": {
    "props": {
      "text": "Envío grátis por compras superiores a 100 USD",
      "blockClass": ["text-banner-home", "text-banner-home-medio"]
    }
  },
  "rich-text#3": {
    "props": {
      "text": "Cambios y devoluciones grátis",
      "blockClass": "text-banner-home"
    }
  },
  "box#banners-secundarios": {
    "props": {
      "blockClass": "banners-secundarios"
    },
    "children": ["media-button#categoria-uno", "media-button#categoria-dos"]
  },
  "media-button#categoria-uno": {
    "props": {
      "mediaType": "image",
      "link": {
        "url": "/sale/ropa"
      },
      "blockClass": ["banner-categoria-uno"],
