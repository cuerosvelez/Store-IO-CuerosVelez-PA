{
  "store.custom#flyup-home": {
    "blocks": ["box#flyup-home"],
    "parent": {
      "header": "condition-header",
      "footer": "nav-footer"
    }
  },
  "box#flyup-home": {
    "children": [
      "media-button#flyup-exa",
      "slider-layout#flyup-banner-text-home",
      "box#flyup-banners-secundarios",
      "media-button#flyup-banner-tres",
      "box#flyup-contenedor-vitrina-home",
      "media-button#flyup-banner-cuatro"
    ]
  },
  "media-button#flyup-exa": {
    "props": {
      "mediaType": "image",
      "link": {
        "url": "/lo-nuevo"
      },
      "blockClass": ["banner-tres-mujer-human-depto"],
      "image": "https://cuerosvelezco.vtexassets.com/assets/vtex.file-manager-graphql/images/69e46431-286c-4633-8035-6529e6ff684b___5ccc69e4ff7490ab78005e7a4e063bd4.jpg",
      "imageMobile": "https://cuerosvelezco.vtexassets.com/assets/vtex.file-manager-graphql/images/070492c1-7048-4fce-867d-dc43ed9d87c3___5760a6258254b857d481be4ecff2cf3f.jpg",
      "buttonList": {
        "listPositionDesk": 2,
        "listPositionMobile": 2,
        "buttons": [
          {
            "__editorItemTitle": "ROPA",
            "name": "DESCRUBRE MÁS",
            "url": "/lo-nuevo/ropa",
            "newTab": false
          }
        ],
        "color": "#ffffff",
        "colorBackground": "#ffffff40",
        "advancedSettings": {
          "showAdvancedSettings": true,
          "marginBottom": 90,
          "marginBottomMobile": 90
        }
      }
    }
  },
  "slider-layout#flyup-banner-text-home": {
    "props": {
      "itemsPerPage": {
        "desktop": 3,
        "tablet": 1,
        "phone": 1
      },
      "infinite": true,
      "showNavigationArrows": "never",
      "showPaginationDots": "never",
      "slideTransition": {
        "speed": 1000,
        "delay": 0,
        "timing": ""
      },
      "autoplay": {
        "timeout": 5000,
        "stopOnHover": true
      },

      "blockClass": "banner-text-home"
    },
    "children": ["rich-text#flyup-1", "rich-text#flyup-2", "rich-text#flyup-3"]
  },
  "rich-text#flyup-1": {
    "props": {
      "text": "Envíos en hasta 3 días hábiles",
      "blockClass": "text-banner-home"
    }
  },
  "rich-text#flyup-2": {
    "props": {
      "text": "Envío gratis por compras superiores a 100 USD",
      "blockClass": ["text-banner-home", "text-banner-home-medio"]
    }
  },
  "rich-text#flyup-3": {
    "props": {
      "text": "Cambios y devoluciones gratis",
      "blockClass": "text-banner-home"
    }
  },
  "box#flyup-banners-secundarios": {
    "props": {
      "blockClass": "banners-secundarios"
    },
    "children": [
      "media-button#flyup-categoria-uno",
      "media-button#flyup-categoria-dos"
    ]
  },
