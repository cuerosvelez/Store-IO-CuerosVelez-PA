  "rich-text#flyup-title-vitrina-home": {
    "props": {
      "text": "## NUESTROS FAVORITOS",
      "blockClass": ["title-vitrina-home"]
    }
  },
  "list-context.product-list#flyup-vitrina-home": {
    "blocks": ["product-summary.shelf"],
    "children": ["slider-layout#flyup-slider-vitrina-home"],
    "props": {
      "orderBy": "OrderByBestDiscountDESC",
      "collection": "974",
      "maxItems": 14
    }
  },
  "slider-layout#flyup-slider-vitrina-home": {
    "props": {
      "itemsPerPage": {
        "desktop": 4,
        "tablet": 2,
        "phone": 2
